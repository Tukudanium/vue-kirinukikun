{
  "name": "vue-kirinukikun",
  "private": false,
  "version": "0.0.0",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/kirinukikun.umd.cjs",
  "module": "./dist/kirinukikun.js",
  "exports": {
    ".": {
      "import": "./dist/kirinukikun.js",
      "require": "./dist/kirinukikun.umd.cjs"
    },
    "./style.css": "./dist/style.css"
  },
  "generate:types": "vue-tsc --declaration --emitDeclarationOnly --outdir ./dist",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint --fix src/*.{ts,vue} && eslint --fix src/**/*.{ts,vue}",
    "format": "prettier -w src/*.{ts,vue} && prettier -w src/**/*.{ts,vue}",
    "fix": "yarn format && yarn lint",
    "deploy": "gh-pages -d dist"
  },
  "dependencies": {
    "vue": "^3.2.41"
  },
  "devDependencies": {
    "@types/node": "^18.11.13",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "@vitejs/plugin-vue": "^3.2.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.26.0",
    "eslint-plugin-vue": "^9.7.0",
    "postcss": "^8.4.18",
    "prettier": "^2.7.1",
    "typescript": "^4.6.4",
    "vite": "^3.2.0",
    "vue-tsc": "^1.0.13"
  },
  "license": "MIT"
}
